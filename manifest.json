{
  "manifest_version": 3,
  "name": "Return Legacy TweetDeck",
  "version": "0.0.0",
  "content_scripts": [
    {
      "matches": ["https://*.twitter.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset",
        "enabled": true,
        "path": "ruleset.json"
      }
    ]
  },
  "host_permissions": ["https://*.twitter.com/*"],
  "permissions": ["declarativeNetRequest"]
}
